# ( .token ) # ·e· 
# show installed API Token # ·e·
alias ".token"="token_setup" # ·a·

# ( .token.setup ) # ·e· 
# PROMPT to add API Token # ·e·
alias ".token.setup"="update_login" # ·a·

# ( .ssh )  # ·e·
# show available ssh keys # ·e·
alias ".ssh"="show_ssh" # ·a·

# ( .ghb.ssh.create ) <ssh-key-name> # ·e·
# send ssh keys to GitHub via API Token # ·e·
alias ".ghb.ssh.create"="add_ssh_github_setup" # ·a·

# ( .repo ) # ·e·
# show Repositories with API Token # ·e·
alias ".repo"="list_repo_setup" # ·a·

# ( .repo.create ) <repo-name> # ·e·

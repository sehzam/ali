alias ".repo.create"="git_setup_and_push_setup" # ·a·

# ( .repo.delete) <repo-name> # ·e·
# delete Repository via API Token # ·e·
alias ".repo.delete"="delete_repo_setup" # ·a·

# ( .repo.collaborator) <repo-name> # ·e·
# show Collaborator on Repository via API Token # ·e·
alias ".repo.collaborator"="list_collaborator_setup" # ·a·

# ( .repo.collaborator.all) # ·e·
# show Collaborator on Repository via API Token # ·e·
alias ".repo.collaborator.all"="list_repo_with_collaborator_setup" # ·a·

# ( .repo.collaborator.add) <repo-name> <collaborator-name> # ·e·
# add Collaborator on Repository via API Token # ·e·
alias ".repo.collaborator.add"="add_collaborator_setup" # ·a·

# ( .repo.collaborator.remove) <repo-name> <collaborator-name> # ·e·
